{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://127.0.0.1:*/*" },
        { "url": "http://127.0.0.1:*/*" }
      ]
    }
  ]
}
